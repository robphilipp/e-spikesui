{
  "name": "spikes-ui",
  "productName": "spikes-ui",
  "version": "1.0.0",
  "description": "User interface for defining and running spikes neural networks",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "test": "jest",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts ."
  },
  "keywords": [],
  "author": {
    "name": "Robert Philipp",
    "email": "rob.philipp@gmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "spikes_ui"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/app.tsx",
                  "name": "main_window"
                }
              ]
            },
            "electronWebpack": {
              "whiteListedModules": [
                "threads"
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@babel/types": "^7.15.6",
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "@electron-forge/plugin-webpack": "^6.0.0-beta.54",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@types/d3": "^7.1.0",
    "@types/react": "^17.0.30",
    "@types/react-dom": "^17.0.10",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-dom": "^5.1.5",
    "@types/three": "^0.133.1",
    "@types/tinycolor2": "^1.4.2",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "copy-webpack-plugin": "^6.4.1",
    "css-loader": "^5.0.1",
    "electron": "^11.1.0",
    "eslint": "^7.14.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "fork-ts-checker-webpack-plugin": "^5.2.0",
    "monaco-editor-webpack-plugin": "^5.0.0",
    "node-loader": "^1.0.2",
    "redux-devtools": "^3.7.0",
    "redux-devtools-extension": "^2.13.8",
    "style-loader": "^1.3.0",
    "threads-plugin": "^1.4.0",
    "ts-jest": "^27.0.7",
    "ts-loader": "^8.0.12",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "@fluentui/react": "^8.37.0",
    "@fluentui/react-cards": "^0.200.44",
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-regular-svg-icons": "^5.15.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.12",
    "@nx-js/compiler-util": "^2.0.0",
    "@types/jest": "^27.0.2",
    "axios": "^0.21.0",
    "d3": "^7.1.1",
    "electron-squirrel-startup": "^1.0.0",
    "file-loader": "^6.1.1",
    "immutable": "^4.0.0",
    "jest": "^27.3.1",
    "moment": "^2.29.1",
    "monaco-editor": "^0.29.1",
    "prelude-ts": "^1.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.2",
    "react-resizable-grid-layout": "^0.2.1",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "rxjs": "^6.6.3",
    "sass-loader": "^10.1.0",
    "stream-charts": "^1.0.1",
    "threads": "^1.6.3",
    "three": "^0.133.1",
    "tinycolor2": "^1.4.2",
    "url-loader": "^4.1.1"
  }
}
